RUN mkdir -p build && cd build && cmake ../dextool -DCMAKE_BUILD_TYPE=Release -DLOW_MEM=ON -DLIBLLVM_VERSION="LLVM_11_0_0" -DLIBLLVM_MAJOR_VERSION="11" -DLIBLLVM_LIBS="-lLLVM-11" -DLIBCLANG_LDFLAGS="-Wl,--enable-new-dtags -Wl,--no-as-needed -L/usr/lib64/llvm11/lib -Wl,-rpath,/usr/lib64/llvm11/lib" -DLIBCLANG_LIBS="-lclang-cpp -lclang"
